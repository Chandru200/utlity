{
  "manifest_version": 3,
  "name": "ProductivityMaster",
  "version": "1.2",
  "key":"",
  "description": "Very useful for day to day works",
  "icons": {
    "16": "images/dog.png",
    "32": "images/dog2.png",
    "48": "images/dog3.png",
    "128": "images/dog4.png"
  },
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": "images/dog.png"
  },
  "background": {
    "service_worker": "background.js",
    "module": true
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content_bundle.js"]
    }
  ],
  "permissions": ["activeTab", "tabs", "storage","identity"],
  "web_accessible_resources": [
    {
      "resources": ["*"],
      "matches": ["<all_urls>"]
    }
  ],
  "oauth2": {
    "client_id": "401688149190-g79r37jid6jfh7nhgtl2l3h7f57h9kp4.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/userinfo.email",
    "https://www.googleapis.com/auth/calendar",
    "https://www.googleapis.com/auth/calendar.events"
  ]
  }
}
